{
  "include": [
    "src/default/assets/js/src/**/*.ts"
  ],
  "compilerOptions": {
    "target": "ES5",
    "module": "ES2015",
    "lib": [
      "ES2016",
      "DOM",
      "DOM.Iterable"
    ],
    "declaration": false,
    "skipLibCheck": true,
    "sourceMap": false,
    "strict": true,
    "alwaysStrict": false,
    "noImplicitUseStrict": true
  }
}
