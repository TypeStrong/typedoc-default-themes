<section class="tsd-panel tsd-member {{cssClasses}}">
    <a name="{{anchor}}" class="tsd-anchor"></a>
    {{#if name}}
        <h3>{{#compact}}{{#each (relevantFlags)}}{{> flag}}&nbsp;{{/each}}{{/compact}}{{{wbr name}}}</h3>
    {{/if}}

    {{#if signatures}}
        {{> member.signatures}}
    {{else}}{{#if hasGetterOrSetter}}
        {{> member.getterSetter}}
    {{else}}
        {{> member.declaration}}
    {{/if}}{{/if}}

    {{#each groups}}
        {{#each children}}
            {{#unless hasOwnDocument}}
                {{> member}}
            {{/unless}}
        {{/each}}
    {{/each}}
</section>
